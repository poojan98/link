<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/mtext1"
        android:layout_width="133dp"
        android:layout_height="41dp"
        android:layout_alignBaseline="@+id/medit1"
        android:layout_alignEnd="@+id/mtext4"
        android:text="MemberID"
        android:textColor="#00FF00"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="34dp" />

    <TextView
        android:id="@+id/mtext2"
        android:layout_width="122dp"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/medit2"
        android:layout_alignEnd="@+id/mtext4"
        android:text="HouseID"
        android:textColor="#00FF00"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="34dp" />

    <TextView
        android:id="@+id/mtext3"
        android:layout_width="103dp"
        android:layout_height="42dp"
        android:layout_alignBaseline="@+id/medit4"
        android:layout_alignStart="@+id/stext"
        android:text="Name"
        android:textColor="#00FF00"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="34dp" />

    <TextView
        android:id="@+id/mtext4"
        android:layout_width="wrap_content"
        android:layout_height="37dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="163dp"
        android:layout_marginStart="21dp"
        android:text="MemberType"
        android:textColor="#00FF00"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="34dp" />


    <TextView
        android:id="@+id/stext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="31dp"
        android:text="Member"
        android:textColor="#FF0000"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="34dp" />


    <EditText
        android:id="@+id/medit1"
        android:layout_width="190dp"
        android:layout_height="58dp"
        android:layout_alignParentEnd="true"
        android:layout_alignParentTop="true"
        android:layout_marginEnd="34dp"
        android:layout_marginTop="92dp" />

    <EditText
        android:id="@+id/medit2"
        android:layout_width="190dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/medit1"
        android:layout_marginTop="165dp" />


    <EditText
        android:id="@+id/medit3"
        android:layout_width="195dp"
        android:layout_height="33dp"
        android:layout_alignStart="@+id/medit1"
        android:layout_alignTop="@+id/mtext4" />

    <EditText
        android:id="@+id/medit4"
        android:layout_width="198dp"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/medit2"
        android:layout_below="@+id/medit2"
        android:layout_marginTop="26dp" />

    <Button
        android:id="@+id/mbutton"
        android:layout_width="124dp"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/mbtnv"
        android:layout_toStartOf="@+id/medit1"
        android:text="Register"
        android:textSize="20sp" />

    <Button
        android:id="@+id/mbtnv"
        android:layout_width="111dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:layout_marginBottom="78dp"
        android:layout_marginEnd="77dp"
        android:text="view" />

    <Button
        android:id="@+id/mbtnd"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="13dp"
        android:layout_toEndOf="@+id/stext"
        android:text="delete" />


</RelativeLayout>