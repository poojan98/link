<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/stext1"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/stext"
        android:layout_marginStart="-5dp"
        android:layout_marginTop="163dp"
        android:text="SellID"
        android:textColor="#00FF00"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="34dp" />

    <TextView
        android:id="@+id/stext2"
        android:layout_width="122dp"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/stext1"
        android:layout_marginTop="242dp"
        android:text="HouseID"
        android:textColor="#00FF00"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="34dp" />

    <TextView
        android:id="@+id/stext3"
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignStart="@+id/stext1"
        android:layout_marginBottom="241dp"
        android:text="Price"
        android:textColor="#00FF00"
        android:textSize="20sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="34dp" />


    <TextView
        android:id="@+id/stext"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="29dp"
        android:layout_marginTop="31dp"
        android:text="Sell"
        android:textColor="#FF0000"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="59dp"
        tools:layout_editor_absoluteY="34dp" />


    <EditText
        android:id="@+id/sedit1"
        android:layout_width="188dp"
        android:layout_height="72dp"
        android:layout_alignBaseline="@+id/stext1"
        android:layout_alignParentEnd="true" />

    <EditText
        android:id="@+id/sedit2"
        android:layout_width="186dp"
        android:layout_height="72dp"
        android:layout_alignBottom="@+id/stext2"
        android:layout_toEndOf="@+id/sedit1" />


    <EditText
        android:id="@+id/sedit3"
        android:layout_width="186dp"
        android:layout_height="72dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="255dp"
        android:layout_toEndOf="@+id/sedit1" />

    <Button
        android:id="@+id/sbutton"
        android:layout_width="153dp"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="112dp"
        android:layout_marginStart="113dp"
        android:text="Sell"
        android:textSize="20sp" />


</RelativeLayout>